
@model DoctorSheduleViewModel

@{
    ViewData["Title"] = "EditDoctorShedule";
    ViewBag.Title = "Edit DoctorShedule";
}

    <form enctype="multipart/form-data" asp-controller="Employees" asp-action="EditDoctorShedule" method="post" class="mt-3">
        <div asp-validation-summary="All" class="text-danger"></div>
        <div class="form-group row">
            <input hidden asp-for="Id" />
            <input hidden asp-for="EmployeeId" />

        </div>
        <div class="form-group">
            <label asp-for="Name"></label>
            <input asp-for="Name" class="form-control" readonly="@true" />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>
        <div class="form-group row">
            <label asp-for="Day" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Day" class="form-control" placeholder="BirthDate" readonly="@false" />
                @*<span asp-validation-for="JoiningDate" class="text-danger"></span>*@
            </div>
        </div>
        <div class="form-group row">
            <label asp-for="Time" class="col-sm-2 col-form-label"></label>
            <div class="col-sm-10">
                <input asp-for="Time" class="form-control" placeholder="BirthDate" readonly="@false" />
                @*<span asp-validation-for="JoiningDate" class="text-danger"></span>*@
            </div>
        </div>

        <div asp-validation-summary="All" class="text-danger"></div>
        <div class="form-group row">
            <div class="col-sm-10">
                <button type="submit" class="btn btn-primary">Upate</button>
                <a asp-controller="Employees" asp-action="ListDoctorShedule" class="btn btn-primary">Cancel</a>
            </div>
        </div>

    </form>
